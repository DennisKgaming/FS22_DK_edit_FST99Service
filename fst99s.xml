<?xml version="1.0" encoding="utf-8" standalone="no" ?>
<vehicle type="fieldServiceTrailerStrobes" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../shared/xml/schema/vehicle.xsd">
    <storeData>
        <name>FST 99S</name>
        <specs>
            <combination xmlFilename="$data/vehicles/lizard/pickup1986/pickup1986.xml" />
            <combination xmlFilename="$data/vehicles/lizard/pickup2017/pickup2017.xml" />
        </specs>
        <functions>
            <function>$l10n_function_serviceTrailer</function>
        </functions>
        <image>store_fst99s.dds</image>
        <price>24500</price>
        <lifetime>600</lifetime>
        <rotation>0</rotation>
        <brand>THUNDERCREEK</brand>
        <category>misc</category>
        <shopTranslationOffset>0 0.055 0</shopTranslationOffset>
        <shopRotationOffset>0 0 0</shopRotationOffset>
    </storeData>

    <base>
        <typeDesc>$l10n_typeDesc_serviceTrailer</typeDesc>
        <filename>fst99s.i3d</filename>

        <size width="2.5" length="5.5" />

        <components maxMass="6000">
            <component centerOfMass="0 0.6 -0.8" solverIterationCount="10" mass="1490" />
        </components>

        <schemaOverlay attacherJointPosition="0 0" name="IMPLEMENT" />
        <mapHotspot type="TRAILER" />
    </base>

    <wheels>
        <wheelConfigurations>
            <wheelConfiguration name="$l10n_configuration_valueDefault" price="0" brand="CONTINENTAL" saveId="CONTINENTAL_DEFAULT">
                <wheels>
                    <wheel filename="$data/shared/wheels/tires/continental/terrainContactHT/255_70R17.xml" configId="trailer" isLeft="true" hasTireTracks="true" >
                        <physics tipOcclusionAreaGroupId="1" restLoad="0.5" repr="wheelFrontLeft"  forcePointRatio="0.35"  initialCompression="20" suspTravel="0.15" spring="14" damper="50" frictionScale="2"/>
                        <tire isCareWheel="false"/>
                    </wheel>
                    <wheel filename="$data/shared/wheels/tires/continental/terrainContactHT/255_70R17.xml" configId="trailer" isLeft="false" hasTireTracks="true" >
                        <physics tipOcclusionAreaGroupId="2" restLoad="0.5" repr="wheelFrontRight" forcePointRatio="0.35"  initialCompression="20" suspTravel="0.15" spring="14" damper="50" frictionScale="2"/>
                        <tire isCareWheel="false"/>
                    </wheel>
                    <wheel filename="$data/shared/wheels/tires/continental/terrainContactHT/255_70R17.xml" configId="trailer" isLeft="true" hasTireTracks="true" hasParticles="true" >
                        <physics tipOcclusionAreaGroupId="1" restLoad="0.5" repr="wheelBackLeft"   forcePointRatio="0.35"  initialCompression="20" suspTravel="0.15" spring="14" damper="50" frictionScale="2"/>
                        <tire isCareWheel="false"/>
                    </wheel>
                    <wheel filename="$data/shared/wheels/tires/continental/terrainContactHT/255_70R17.xml" configId="trailer" isLeft="false" hasTireTracks="true" hasParticles="true" >
                        <physics tipOcclusionAreaGroupId="2" restLoad="0.5" repr="wheelBackRight"  forcePointRatio="0.35"  initialCompression="20" suspTravel="0.15" spring="14" damper="50" frictionScale="2"/>
                        <tire isCareWheel="false"/>
                    </wheel>
                </wheels>
            </wheelConfiguration>
            <wheelConfiguration name="$l10n_configuration_valueDefault" price="0" brand="NOKIAN" saveId="NOKIAN_DEFAULT">
                <wheels baseConfig="CONTINENTAL_DEFAULT">
                    <wheel filename="$data/shared/wheels/tires/nokian/TRI2/250_75R16.xml">
                        <physics yOffset="-0.004"/>
                    </wheel>
                    <wheel filename="$data/shared/wheels/tires/nokian/TRI2/250_75R16.xml">
                        <physics yOffset="-0.004"/>
                    </wheel>
                    <wheel filename="$data/shared/wheels/tires/nokian/TRI2/250_75R16.xml">
                        <physics yOffset="-0.004"/>
                    </wheel>
                    <wheel filename="$data/shared/wheels/tires/nokian/TRI2/250_75R16.xml">
                        <physics yOffset="-0.004"/>
                    </wheel>
                </wheels>
            </wheelConfiguration>
        </wheelConfigurations>
    </wheels>

    <dynamicallyLoadedParts>
        <dynamicallyLoadedPart node="0" linkNode="extinguisherLinkNode"       filename="$data/shared/assets/extinguisher.i3d"/>

        <dynamicallyLoadedPart node="0" linkNode="workshopMarkerLinkNode1"    filename="shared/trafficCone.i3d"/>
        <dynamicallyLoadedPart node="0" linkNode="workshopMarkerLinkNode2"    filename="shared/trafficCone.i3d"/>
        <dynamicallyLoadedPart node="0" linkNode="workshopMarkerLinkNode3"    filename="shared/trafficCone.i3d"/>
        <dynamicallyLoadedPart node="0" linkNode="workshopMarkerLinkNode4"    filename="shared/trafficCone.i3d"/>

        <dynamicallyLoadedPart node="0" linkNode="consumablesMarkerLinkNode1" filename="shared/trafficCone.i3d"/>
        <dynamicallyLoadedPart node="0" linkNode="consumablesMarkerLinkNode2" filename="shared/trafficCone.i3d"/>
        <dynamicallyLoadedPart node="0" linkNode="consumablesMarkerLinkNode3" filename="shared/trafficCone.i3d"/>
        <dynamicallyLoadedPart node="0" linkNode="consumablesMarkerLinkNode4" filename="shared/trafficCone.i3d"/>
    </dynamicallyLoadedParts>

    <attachable>
        <inputAttacherJoints>
            <inputAttacherJoint node="attacherJoint" jointType="trailerLow" attacherHeight="0.40"/>
            <inputAttacherJoint node="attacherJointToolbar" jointType="implement" lowerRotLimitScale="3 3 3" upperRotLimitScale="3 3 3" attacherHeight="0.49" needsToolbar="true" allowsLowering="false">
                <distanceToGround lower="0.40" upper="0.40" />
            </inputAttacherJoint>
        </inputAttacherJoints>

        <support animationName="moveSupport" />

        <brakeForce force="0.2" maxForce="0.5" maxForceMass="5700"/>
        <airConsumer usage="0.3" />
    </attachable>

    <connectionHoses>
        <hose inputAttacherJointIndices="1" type="electric" node="electricReferencePoint" length="0.8" diameter="0.015" straighteningFactor="0.8">
            <objectChange node="cab" rotationActive="-12 0 0" rotationInactive="-90 0 0"/>
            <objectChange node="connector" visibilityActive="true" visibilityInactive="false"/>
        </hose>
    </connectionHoses>

    <lights>
        <sharedLight linkNode="rearLightOvalTwinLED_01" filename="shared/rearLightOvalLEDLeft.xml" />
        <sharedLight linkNode="rearLightOvalTwinLED_02" filename="shared/rearLightOvalLEDRight.xml" />

        <sharedLight linkNode="plateLightLeft" filename="$data/shared/assets/lights/lizard/plateNumberLight02.xml" />
        <sharedLight linkNode="plateLightRight" filename="$data/shared/assets/lights/lizard/plateNumberLight02.xml" />

        <realLights>
            <high>
                <light node="backLightsHigh" lightTypes="0" />
                <brakeLight node="backLightsHigh" />

                <turnLightLeft node="turnLightLeftBack" />
                <turnLightRight node="turnLightRightBack" />

                <light node="licensePlateLightHigh" lightTypes="0" />
            </high>
        </realLights>
    </lights>

    <lightExtension>
        <strobeLights>
            <strobeLight linkNode="strobeLight1" filename="shared/strobeLightOrange.xml" realLightNode="strobeLight1_Real" realLightRange="3.6" blinkStepLength="0.06" blinkPattern="-------X-X-X-X"/>
            <strobeLight linkNode="strobeLight2" filename="shared/strobeLightOrange.xml" realLightNode="strobeLight2_Real" realLightRange="3.6" blinkStepLength="0.06" blinkPattern="X-X-X-X-------"/>
            <strobeLight linkNode="strobeLight3" filename="shared/strobeLightOrange.xml" realLightNode="strobeLight3_Real" realLightRange="3.6" blinkStepLength="0.06" blinkPattern="-------X-X-X-X"/>
            <strobeLight linkNode="strobeLight4" filename="shared/strobeLightOrange.xml" realLightNode="strobeLight4_Real" realLightRange="3.6" blinkStepLength="0.06" blinkPattern="X-X-X-X-------"/>
            <strobeLight linkNode="strobeLight5" filename="shared/strobeLightOrange.xml" realLightNode="strobeLight5_Real" realLightRange="2.4" blinkStepLength="0.05" blinkPattern="X-XXX------"/>
            <strobeLight linkNode="strobeLight6" filename="shared/strobeLightOrange.xml" realLightNode="strobeLight6_Real" realLightRange="2.4" blinkStepLength="0.05" blinkPattern="------X-XXX"/>
        </strobeLights>
    </lightExtension>

    <licensePlates>
        <licensePlate node="licensePlate" position="BACK" preferedType="ELONGATED" placementArea="0.03 0.20 0.16 0.20"/>
    </licensePlates>

    <animations>
        <animation name="moveSupport">
            <part node="support" startTime="0" endTime="0.6" startTrans="0 0.277 1.564" endTrans="0 -0.08 1.564" />
            <part node="supportHandle" startTime="0.0" endTime="0.6" startRot="0 0 0" endRot="0 0 -720"/>

            <sound template="clackVar6" startTime="0.01" direction="-1" volumeScale="1" pitchScale="1" linkNode="attacherJoint"/>
        </animation>

        <animation name="fillLevel">
            <part node="fillLevelMarker" startTime="0" endTime="1" startRot="0 0 0" endRot="-100 0 0" />
        </animation>

        <animation name="rearDoor">
            <part node="rearDoor" startTime="0.0" endTime="3.0" startRot="0 0 0" endRot="85 0 0"/>

            <part node="laptopScreen" startTime="2.0" endTime="3.0" startRot="0 0 100" endRot="0 0 10"/>
            <part node="laptop" startTime="2.5" endTime="3.0" startTrans="0.226 1.328 -2.019" endTrans="0.223 1.328 -2.15" />

            <part node="laptopScreenBacklight_01" startTime="0.0" endTime="3.1" visibility="false" />
            <part node="laptopScreenBacklight_01" startTime="3.1" endTime="3.2" visibility="true" />

            <part node="sellAreaTriggerAnimation" startTime="3.0" endTime="3.1" startTrans="0 -30 0" endTrans="0 0 0" />

            <part node="workLightStatic_rear" startTime="3.1" endTime="3.2" shaderParameter="lightControl" shaderStartValues="0 0 0 0" shaderEndValues="5 0 0 0" />
            <part node="workLightReal_rear" startTime="0.0" endTime="3.1" visibility="false" />
            <part node="workLightReal_rear" startTime="3.1" endTime="3.2" visibility="true" />

            <part node="workLightStatic_right" startTime="3.1" endTime="3.2" shaderParameter="lightControl" shaderStartValues="0 0 0 0" shaderEndValues="80 0 0 0" />
            <part node="workLightReal_right" startTime="0.0" endTime="3.1" visibility="false" />
            <part node="workLightReal_right" startTime="3.1" endTime="3.2" visibility="true" />
        </animation>

        <animation name="fuelPump">
            <part node="workLightStatic_left" startTime="0.0" endTime="0.5" shaderParameter="lightControl" shaderStartValues="0 0 0 0" shaderEndValues="10 0 0 0" />
            <part node="workLightStatic_left" startTime="0.5" endTime="1.0" shaderParameter="lightControl" shaderStartValues="10 0 0 0" shaderEndValues="80 0 0 0" />

            <part node="workLightReal_left" startTime="0.0" endTime="0.5" visibility="false" />
            <part node="workLightReal_left" startTime="0.5" endTime="1.0" visibility="true" />

            <part node="consumablesTriggerAnimation" startTime="0.0" endTime="1.0" startTrans="0 -30 0" endTrans="0 0 0" />
        </animation>
    </animations>

    <cylindered>
        <movingParts>
            <movingPart node="hydrulicRams" referencePoint="hydraulicRef" referenceFrame="hydraulicRef">
                <translatingPart node="hydrulicPunches"/>
            </movingPart>
        </movingParts>

        <movingTools>
            <movingTool node="rearDoor">
                <dependentPart node="hydrulicRams"/>
            </movingTool>
        </movingTools>
    </cylindered>

    <service>
        <workshop playerTriggerNode="sellTrigger" vehicleTriggerNode="sellAreaTrigger" >
            <operation animationName="rearDoor" openSpeedScale="1" closeSpeedScale="-0.8" activateText="$l10n_action_activateService" deactivateText="$l10n_action_deactivateService"/>

            <infoTexts visibilityNode="laptopScreenBacklight_02">
                <headers>
                    <value node="valueHeader" textSize="0.012" />
                    <age node="ageHeader" textSize="0.012" />
                    <operatingHours node="operatingHoursHeader" textSize="0.012" />
                    <condition node="conditionHeader" textSize="0.012" />
                    <paintCondition node="paintConditionHeader" textSize="0.012" />
                </headers>

                <values>
                    <name node="vehicleNameText" textSize="0.016" textColour="0.9301 0.2874 0.0130 1"/>
                    <value node="valueText" textSize="0.01" />
                    <age node="ageText" textSize="0.01" />
                    <operatingHours node="operatingHoursText" textSize="0.01" />
                    <condition node="conditionText" textSize="0.01" />
                    <paintCondition node="paintConditionText" textSize="0.01" />
                </values>
            </infoTexts>

            <triggerMarkers hideWhenMoving="true" activateText="$l10n_action_showMarkers" deactivateText="$l10n_action_hideMarkers">
                <triggerMarker node="workshopMarkerGlow" shaderParameter="colorScale" shaderOffValues="1 0 0 1" shaderOnValues="0 0 1 1" adjustToGround="false"/>

                <triggerMarker node="workshopMarkerLinkNode1" activeNode="workshopMarkerActive1" inactiveNode="workshopMarkerInactive1" adjustToGround="true"/>
                <triggerMarker node="workshopMarkerLinkNode2" activeNode="workshopMarkerActive2" inactiveNode="workshopMarkerInactive2" adjustToGround="true" />
                <triggerMarker node="workshopMarkerLinkNode3" activeNode="workshopMarkerActive3" inactiveNode="workshopMarkerInactive3" adjustToGround="true"/>
                <triggerMarker node="workshopMarkerLinkNode4" activeNode="workshopMarkerActive4" inactiveNode="workshopMarkerInactive4" adjustToGround="true"/>
            </triggerMarkers>
        </workshop>

        <consumables playerTriggerNode="consumablesPlayerTrigger" vehicleTriggerNode="consumablesTrigger" >
            <operation animationName="fuelPump" openSpeedScale="1" closeSpeedScale="-1" activateText="$l10n_action_startMotor" deactivateText="$l10n_action_stopMotor">
                <!-- Consume diesel while idle running, adjusts to pumping or charging value as needed. -->
                <engine fillUnitIndex="1" usagePerSecond="0.005">
                    <exhaustEffect node="exhaustNode" filename="$data/effects/exhaust/exhaust.i3d" minLoadColor="0.5 0.5 0.5 0.4" maxLoadColor="0 0 0 1" minLoadScale="0.08" maxLoadScale="0.2" />
                </engine>

                <dashboards>
                    <dashboard displayType="SLIDER" valueType="pumpRate"  node="loadBarTransfer"/>
                    <dashboard displayType="SLIDER" valueType="battery"   node="loadBarBattery"/>
                    <dashboard displayType="SLIDER" valueType="engineRpm" node="loadBarRPM"/>
                </dashboards>

                <animationNodes>
                    <animationNode node="fan" rotSpeed="-1000" rotAxis="1" turnOnFadeTime="1" turnOffFadeTime="2"/>
                </animationNodes>

                <sounds>
                    <toggleConsumable file="$data/sounds/vehicles/switch/jd_blinker.wav" linkNode="toggleSoundLinkNode" innerRadius="5.0" outerRadius="40.0" >
                        <volume indoor="0.8" outdoor="0.8" />
                        <pitch indoor="1.00" outdoor="1.00" />
                        <lowpassGain indoor="1.00" outdoor="0.50" />
                    </toggleConsumable>

                    <start file="$data/vehicles/aebi/cc66/sounds/aebi_cc66_start.wav"    linkNode="exhaustNode" innerRadius="10.0" outerRadius="80.0" >
                        <volume indoor="0.95" outdoor="0.95" />
                        <lowpassGain indoor="1.0" outdoor="1.0" />
                    </start>

                    <work file="$data/vehicles/aebi/cc66/sounds/aebi_cc66_idle_loop.wav" linkNode="exhaustNode" innerRadius="10.0" outerRadius="80.0" >
                        <volume indoor="0.95" outdoor="0.95" >
                            <modifier type="SERVICE_ENGINE_LOAD" value="0.00" modifiedValue="1.00" />
                            <modifier type="SERVICE_ENGINE_LOAD" value="1.00" modifiedValue="0.50" />
                        </volume>
                        <pitch indoor="1.0" outdoor="1.0" >
                            <modifier type="SERVICE_ENGINE_LOAD" value="0.00" modifiedValue="1.00" />
                            <modifier type="SERVICE_ENGINE_LOAD" value="1.00" modifiedValue="1.154" />
                        </pitch>
                    </work>

                    <work file="$data/vehicles/aebi/cc66/sounds/aebi_cc66_run_loop.wav" linkNode="exhaustNode" innerRadius="10.0" outerRadius="80.0" >
                        <volume indoor="0.95" outdoor="0.95" >
                            <modifier type="SERVICE_ENGINE_LOAD" value="0.00" modifiedValue="0.00" />
                            <modifier type="SERVICE_ENGINE_LOAD" value="0.01" modifiedValue="0.30" />
                            <modifier type="SERVICE_ENGINE_LOAD" value="0.10" modifiedValue="0.50" />
                            <modifier type="SERVICE_ENGINE_LOAD" value="1.00" modifiedValue="1.00" />
                        </volume>
                        <pitch indoor="1.0" outdoor="1.0" >
                            <modifier type="SERVICE_ENGINE_LOAD" value="0.00" modifiedValue="0.66" />
                            <modifier type="SERVICE_ENGINE_LOAD" value="1.00" modifiedValue="0.86" />
                        </pitch>
                    </work>

                    <stop file="$data/vehicles/aebi/cc66/sounds/aebi_cc66_stop.wav" linkNode="exhaustNode" innerRadius="10.0" outerRadius="80.0" />
                </sounds>
            </operation>

            <triggerMarkers hideWhenMoving="true" activateText="$l10n_action_showMarkers" deactivateText="$l10n_action_hideMarkers">
                <triggerMarker node="consumablesMarkerGlow" shaderParameter="colorScale" shaderOffValues="1 0 0 1" shaderOnValues="0 0 1 1" adjustToGround="false"/>

                <triggerMarker node="consumablesMarkerLinkNode1" activeNode="consumablesMarkerActive1" inactiveNode="consumablesMarkerInactive1" adjustToGround="true"/>
                <triggerMarker node="consumablesMarkerLinkNode2" activeNode="consumablesMarkerActive2" inactiveNode="consumablesMarkerInactive2" adjustToGround="true" />
                <triggerMarker node="consumablesMarkerLinkNode3" activeNode="consumablesMarkerActive3" inactiveNode="consumablesMarkerInactive3" adjustToGround="true"/>
                <triggerMarker node="consumablesMarkerLinkNode4" activeNode="consumablesMarkerActive4" inactiveNode="consumablesMarkerInactive4" adjustToGround="true"/>
            </triggerMarkers>

            <sharedFillSamples>
                <sharedFillSample fillTypes="ELECTRICCHARGE">
                    <sample template="ELECTRIC_CHARGING_STATION" innerRadius="4" outerRadius="30" volumeScale="1.5" pitchScale="1" />
                </sharedFillSample>
            </sharedFillSamples>

            <serviceConsumablesConfigurations>
                <serviceConsumablesConfiguration>
                    <consumablesTypes>
                        <consumablesType variation="fillUnit"  title="$l10n_fillType_diesel" fillUnitIndex="1" litersPerSecond="10" usagePerSecond="0.01">
                            <objectChange node="dieselIcon" visibilityActive="true"/>
                            <objectChange node="defIcon" visibilityActive="false"/>
                            <objectChange node="electricIcon" visibilityActive="false"/>
                        </consumablesType>

                        <consumablesType variation="generator" title="$l10n_fillType_electricCharge" litersPerSecond="0.08888" usagePerSecond="0.014">
                            <objectChange node="dieselIcon" visibilityActive="false"/>
                            <objectChange node="defIcon" visibilityActive="false"/>
                            <objectChange node="electricIcon" visibilityActive="true"/>
                        </consumablesType>
                    </consumablesTypes>
                </serviceConsumablesConfiguration>

                <serviceConsumablesConfiguration>
                    <consumablesTypes>
                        <consumablesType variation="fillUnit" title="$l10n_fillType_diesel"    fillUnitIndex="1" litersPerSecond="10" usagePerSecond="0.01">
                            <objectChange node="dieselIcon" visibilityActive="true"/>
                            <objectChange node="defIcon" visibilityActive="false"/>
                            <objectChange node="electricIcon" visibilityActive="false"/>
                        </consumablesType>
                        <consumablesType variation="fillUnit" title="$l10n_fillType_def_short" fillUnitIndex="2" litersPerSecond="10" usagePerSecond="0.01">
                            <objectChange node="dieselIcon" visibilityActive="false"/>
                            <objectChange node="defIcon" visibilityActive="true"/>
                            <objectChange node="electricIcon" visibilityActive="false"/>
                        </consumablesType>
                        <consumablesType variation="generator" title="$l10n_fillType_electricCharge" litersPerSecond="0.08888" usagePerSecond="0.015">
                            <objectChange node="dieselIcon" visibilityActive="false"/>
                            <objectChange node="defIcon" visibilityActive="false"/>
                            <objectChange node="electricIcon" visibilityActive="true"/>
                        </consumablesType>
                    </consumablesTypes>
                </serviceConsumablesConfiguration>
            </serviceConsumablesConfigurations>
        </consumables>
    </service>

    <fillUnit>
        <fillUnitConfigurations>
            <fillUnitConfiguration>
                <fillUnits>
                    <fillUnit fillTypes="diesel" capacity="2875" showInShop="true">
                        <dashboard displayType="ANIMATION"  valueType="fillLevel" animName="fillLevel"/>

                        <dashboard displayType="SLIDER"     valueType="fillLevel" node="loadBarFuel"/>
                        <dashboard displayType="TEXT"       valueType="fillLevel" node="numbersFillLevel" groups="ENGINE_RUNNING" textColor="0.19 0.002 0.001 1" emissiveScale="4" textSize="0.038" textMask="0000" font="DIGIT"/>

                        <dashboard displayType="VISIBILITY" valueType="fillLevelWarning" node="dangerousGoodsSign_01"  warningThresholdMin="1"/>
                        <dashboard displayType="VISIBILITY" valueType="fillLevelWarning" node="dangerousGoodsSign_02"  warningThresholdMin="1"/>
                    </fillUnit>
                </fillUnits>
            </fillUnitConfiguration>

            <fillUnitConfiguration>
                <fillUnits>
                    <fillUnit fillTypes="diesel" capacity="2000" showInShop="true">
                        <dashboard displayType="ANIMATION"  valueType="fillLevel" animName="fillLevel"/>

                        <dashboard displayType="SLIDER"     valueType="fillLevel" node="loadBarFuel"/>
                        <dashboard displayType="TEXT"       valueType="fillLevel" node="numbersFillLevel" groups="ENGINE_RUNNING" textColor="0.19 0.002 0.001 1" emissiveScale="4" textSize="0.038" textMask="0000" font="DIGIT"/>

                        <dashboard displayType="VISIBILITY" valueType="fillLevelWarning" node="dangerousGoodsSign_01"  warningThresholdMin="1"/>
                        <dashboard displayType="VISIBILITY" valueType="fillLevelWarning" node="dangerousGoodsSign_02"  warningThresholdMin="1"/>
                    </fillUnit>

                    <!-- Reduce the storage tank to 2000l and add 850l DEF tank also. 75 l loss of size due to baffles etc-->
                    <fillUnit fillTypes="def" capacity="850" showInShop="true"/>
                </fillUnits>
            </fillUnitConfiguration>
        </fillUnitConfigurations>
    </fillUnit>

    <dashboard>
        <groups>
            <group name="ENGINE_STARTING" isServiceEngineStarting="true"/>
            <group name="ENGINE_ACTIVE"   isServiceEngineStarting="true" isServiceEngineRunning="true"/>
            <group name="ENGINE_RUNNING"  isServiceEngineRunning="true"/>
        </groups>

        <default>
            <dashboard displayType="VISIBILITY" node="dashboardDecal"        groups="ENGINE_RUNNING" />
            <dashboard displayType="VISIBILITY" node="dashboardStartupDecal" groups="ENGINE_STARTING" />
            <dashboard displayType="VISIBILITY" node="dashboardBacklight"    groups="ENGINE_RUNNING" />
        </default>
    </dashboard>

    <tipOccluder>
        <occlusionArea start="tipOcclusionAreaStart" width="tipOcclusionAreaWidth" height="tipOcclusionAreaHeight" />
    </tipOccluder>

    <foliageBending>
        <bendingNode minX="-0.9" maxX="0.9" minZ="-2.8" maxZ="2.6" yOffset="0.3" />
        <bendingNode minX="-1.3" maxX="1.3" minZ="-1.9" maxZ="0.1" yOffset="0.3" />
    </foliageBending>

    <ai>
        <agentAttachment jointNode="attacherJoint" rotCenterWheelIndices="1 2 3 4" width="2.1" height="2" length="4.6" lengthOffset="-0.1"/>
    </ai>

    <configurationSets>
        <configurationSet price="0" name="$l10n_configuration_valueDefault">
            <configuration index="1" name="serviceConsumables"/>
            <configuration index="1" name="fillUnit"/>
        </configurationSet>

        <configurationSet price="175" name="+ DEF">
            <configuration index="2" name="serviceConsumables"/>
            <configuration index="2" name="fillUnit"/>
        </configurationSet>
    </configurationSets>

    <designConfigurations title="$l10n_configuration_glowingMarkers">
        <designConfiguration name="$l10n_configuration_valueYes" price="0">
            <objectChange node="workshopMarkerGlowConfig" visibilityActive="true"/>
            <objectChange node="consumablesMarkerGlowConfig" visibilityActive="true"/>
        </designConfiguration>

        <designConfiguration name="$l10n_configuration_valueNo" price="0">
            <objectChange node="workshopMarkerGlowConfig" visibilityActive="false"/>
            <objectChange node="consumablesMarkerGlowConfig" visibilityActive="false"/>
        </designConfiguration>
    </designConfigurations>

    <vehicleTypeConfigurations title="$l10n_configuration_strobeLights">
        <vehicleTypeConfiguration name="$l10n_configuration_valueYes" price="0" vehicleType="fieldServiceTrailerStrobes">
            <objectChange node="strobeLights" visibilityActive="true"/>
        </vehicleTypeConfiguration>

        <vehicleTypeConfiguration name="$l10n_configuration_valueNo" price="0" vehicleType="fieldServiceTrailer">
            <objectChange node="strobeLights" visibilityActive="false"/>
        </vehicleTypeConfiguration>
    </vehicleTypeConfigurations>

    <rimColorConfigurations useDefaultColors="false" >
